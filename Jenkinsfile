pipeline {
    agent any
    stages {
        stage("hello") {
            steps {
                echo "welcome to Jenkins pipeline"
            }
        }
        stage("TEST BUILD") {
            steps {
                echo "BUILD SUCCESS"
            }
        }
        stage("DEPLOY BUILD") {
            steps {
                echo "DEPLOY SUCCESS"
            }
        }
        stage("UPLOAD BUILD DATA") {
            steps {
                echo "BUILD DATA UPLOADED"
            }
        }
      
        
        stage("Ended") {
            steps {
                echo "BUILD DATA UPLOADED"
            }
        }
    }
}
